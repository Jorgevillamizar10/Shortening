{
  "version": 2,
  "name": "Shorty",
  "builds": [
    {
      "use": "@vercel/static-build",
      "src": "package.json"
    }
  ],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "(.*).jsx",
      "dest": "/$1.jsx"
    },
    {
      "src": "(.*).scss",
      "dest": "/$1.scss"
    },
    {
      "src": "(.*).css",
      "dest": "/$1.css"
    },
    {
      "src": "(.*).json",
      "dest": "/$1.json"
    },
    {
      "src": "(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "(.*).svg",
      "dest": "/$1.svg"
    },
    {
      "src": "(.*).jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
